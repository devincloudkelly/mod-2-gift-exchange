<h1><%= @wish.user.name %>'s wish</h1>

<h3>Item: <%= @wish.item.item_name %></h3>
<h3><%= @wish.user.name %>'s notes: <%= @wish.notes %></h3>
<h3>Quantity: <%= @wish.qty %></h3>
<h3><%= link_to "Link to #{@wish.item.item_name}", @wish.item.item_url %></h3>
<h3><%= link_to "Back to #{@wish.user.name}'s Wishlist", @wish.user %></h3>

<%= button_to "Delete Wish", @wish, :method => :delete %>